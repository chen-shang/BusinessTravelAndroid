<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.activity.project.fragment.ProjectFragment"
        android:orientation="vertical">
    <!--头部绿色区域-->
    <com.business.travel.app.view.ContentBar
            android:id="@+id/topTitleBar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:content_bar_left_icon="@drawable/ic_app_icon"
            app:content_bar_title="@string/app_name"
            app:content_bar_right_icon="@drawable/ic_base_other"
            />

    <com.yanzhenjie.recyclerview.SwipeRecyclerView
            android:id="@+id/UI_ProjectFragment_SwipeRecyclerView_ProjectList"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
</LinearLayout>